{
	"_version": "1.12.0",
	"sap.app": {
		"id": "in.sijas.app.ui5.voicerecognition.VoiceRecognition",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"Northwind": {
				"uri": "/NorthWind_Service/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"config": {
			"sapFiori2Adaptation": true
		},
		"rootView": {
			"viewName": "in.sijas.app.ui5.voicerecognition.VoiceRecognition.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "in.sijas.app.ui5.voicerecognition.VoiceRecognition.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Inline"
				},
				"dataSource": "Northwind",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "in.sijas.app.ui5.voicerecognition.VoiceRecognition.view",
				"controlAggregation": "masterPages",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed": {
					"target": []
				}
			},
			"routes": [
				{
					"name": "master",
					"pattern": "",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"master",
						"detail"
					]
				},
				{
					"name": "detail",
					"pattern": "detail/{context}",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"master",
						"detail"
					]
				},
				{
					"name": "edit",
					"pattern": "edit/{context}",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"master",
						"edit"
					]
				}
			],
			"targets": {
				"master": {
					"viewType": "XML",
					"viewName": "Master"
				},
				"detail": {
					"viewType": "XML",
					"controlAggregation": "detailPages",
					"viewName": "Display"
				},
				"edit": {
					"viewType": "XML",
					"controlAggregation": "detailPages",
					"viewName": "Edit",
					"transition": "flip"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}